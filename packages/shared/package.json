{
  "name": "@hyper-fairy-chess/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./game/*": {
      "import": "./dist/game/*.js",
      "types": "./dist/game/*.d.ts"
    },
    "./protocol": {
      "import": "./dist/protocol/index.js",
      "types": "./dist/protocol/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  }
}
